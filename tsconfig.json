{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "lib": ["es6"],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "dist",
    "paths": {
      "*": [
        "node_modules/*",
      ]
    },
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*.ts"
  ]
}
